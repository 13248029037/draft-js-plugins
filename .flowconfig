[ignore]
.*/node_modules/babel.*
.*/node_modules/editions/.*
.*/node_modules/fbjs/.*
.*/node_modules/immutable/.*
.*/node_modules/.*.json
.*/node_modules/react/.*
.*/node_modules/radium/.*
.*/node_modules/chalk/.*
.*/node_modules/documentation/.*
.*/docs/.*
.*/docs/node_modules/.*

[include]

[libs]
flow-lib

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.system=haste

[version]
^0.69.0
