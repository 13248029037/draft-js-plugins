{
  "name": "draft-js-plugin-editor",
  "version": "0.0.0",
  "description": "A Plugin Architecture on top of Draft.JS",
  "author": {
    "name": "Nik Graf",
    "email": "nik@nikgraf.com",
    "url": "http://www.nikgraf.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikgraf/draft-js-plugin-editor.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "scripts": {
    "build": "BABEL_ENV=production ./node_modules/.bin/babel --out-dir='lib' --ignore='__tests__/*' src",
    "prepublish": "npm run build",
    "postpublish": "./scripts/publish_gh_pages.sh",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --require testHelper.js src/**/__test__/*.js",
    "test:watch": "npm test -- --watch",
    "lint": "npm run lint:eslint && npm run lint:jscs && npm run lint:flow",
    "lint:eslint": "eslint --rule 'mocha/no-exclusive-tests:2' ./",
    "lint:eslint:fix": "eslint --fix --rule 'mocha/no-exclusive-tests:2' ./",
    "lint:jscs": "jscs ./",
    "lint:jscs:src": "jscs src",
    "lint:jscs:examples": "jscs examples --max-old-space-size=4096",
    "lint:jscs:docs": "jscs docs",
    "lint:flow": "flow status",
    "alex": "alex README.md"
  },
  "devDependencies": {
    "alex": "^2.0.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^9.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "belle": "^2.0.4",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-mocha": "^2.0.0",
    "eslint-plugin-react": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "flow-bin": "^0.22.1",
    "jest-cli": "^0.9.0",
    "jscs": "^2.11.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "=2.9.1"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "draft-js-cutting-edge": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "^3.7.6",
    "linkify-it": "1.2.0",
    "tlds": "1.97.0"
  }
}
